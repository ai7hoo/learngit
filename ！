+      setTimeout(function () {
+        // but gets reset when validation fails
+        assertCalls(calls, ['a.canDeactivate'])
+        expect(router.app.$el.textContent).toBe('A ')
+        expect(router.history.currentPath).toBe('/a')
+        done()
