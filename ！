+        <a v-link="{ path:'/bar' }">bar</a>
+      </p>
+      <router-view></router-view>
+    </div>
+    <div id="box">
+      <h2>test</h2>
+      <p>
+        <a v-link="{ path:'/a' }">a</a>
+        <a v-link="{ path:'/b' }">b</a>
+      </p>
+      <router-view></router-view>
+    </div>
+    <script src="http://cdn.bootcss.com/vue/1.0.13/vue.min.js"></script>
+    <script src="http://cdn.bootcss.com/vue-router/0.7.7/vue-router.js"></script>
+    <script src="demo.js"></script>
+  </body>
+</html>
