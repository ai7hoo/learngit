+<script src="/Public/Beauty/js/layer.m.js"></script>
+<script type="text/javascript">
+$('#School').find('li').click(function(){
+    var _relId = parseInt($(this).attr('rel-id'));
+    if (_relId>0) {
+        $.post('/Beauty/Basic/getDormitory',{'filterAm':1,'pid':_relId},function(ret){
+            var _data = ret.data;
+            if (_data) {
+                var _len = _data.length,
+                    _html = '';
+                for (var i=0;i<_len;i++) {
+                    _html += '<li rel-id="'+_data[i].id+'">'+_data[i].title+'</li>';
+                }
+                $('#Dorm').find('.tag_select').html('请选择所在宿舍楼');
+                $('#Dorm').find('.tag_options').html(_html);
+                jQuery("#Dorm").selectCss();
+            }
+            else {
+                layer.open({
+                    content: "该大学暂无宿舍楼",
+                    style: 'background-color: rgba(0,0,0,.6); text-align:center; color:#fff; border:none; font-size:20px; letter-spacing: 4px; line-height: 200%;max-width: 300px',
+                    time: 2
+                });
+            }
+        },'json')
+    }
+})
+var startTime=60,timer;
+function fs(el,is_clear){
+    if (is_clear==1) {
+        $(el).removeAttr("disabled");
+        $(el).removeClass("noClick");
+        $(el).val("发送验证码");
+        clearInterval(timer);
+        return ;
+    }
+    $(el).attr("disabled","disabled");
+    $(el).toggleClass("noClick");
+    $(el).val("60s后重新发送");
+    timer=setInterval(function(){
+        startTime--;
+        if(startTime==0){
+            startTime=60;
+            $(el).removeAttr("disabled");
+            $(el).toggleClass("noClick");
+            $(el).val("发送验证码");
+            clearInterval(timer);
+            return;
+        }
+        $(el).val(startTime+"s后重新发送");    
+    },1000);
+}
+//发送验证码
+$('.codeBtn').click(function(){
+    var _t = $(this),
+        _mobile = $('.mobile').val();
+    fs(_t);
+    $.post('/Beauty/Home/sendTelCode/',{'mobile':_mobile},function(ret){
+        if (ret.code==0) return ;
+        fs(_t,1);
+        layer.open({
+            content: ret.msg,
+            style: 'background-color: rgba(0,0,0,.6); text-align:center; color:#fff; border:none; font-size:20px; letter-spacing: 4px; line-height: 200%;max-width: 300px',
+            time: 2
+        });
+    },'json')
+})
+var args={};
+    $('#go_other').click(function(){
+        args.mobile = $('.mobile').val();
+        args.authcode = $('.authcode').val();
+        args.realname = $('.realname').val();
+        args.contact_mobile = $('.contact_mobile').val();
+        args.bedroom = $('.bedroom').val();
+        args.channel = $('.channel').val();
+        args.dormitory_id = 0;
+        $('#Dorm').find('li').each(function(){
+            if ($(this).hasClass('open_selected')) args.dormitory_id = parseInt($(this).attr('rel-id'));
+        })
+        $.post('/Beauty/Bonus/mysteryGiftBuy',args,function(ret){},'json')
+    })
+    $('#payment_buy').click(function(){
+        args.mobile = $('.mobile').val();
+        args.authcode = $('.authcode').val();
+        args.realname = $('.realname').val();
+        args.contact_mobile = $('.contact_mobile').val();
+        args.bedroom = $('.bedroom').val();
+        args.dormitory_id = 0;
+        var channel = $('.channel').val();
+        $('#Dorm').find('li').each(function(){
+            if ($(this).hasClass('open_selected')) args.dormitory_id = parseInt($(this).attr('rel-id'));
+        })
+        if (!channel) channel = 'wolegequya';
+        $.post('/Beauty/Bonus/mysteryGiftBuy',args,function(ret){
+            if (ret.code==0) {
+                $.post('/Beauty/Mall/orderBuy',{'remark':"神秘礼盒代付",'is_payment':1,'channel':channel},
+                function(ret){
+                    if (ret.code==0) {
+                        window.location.href = ret.url;
+                        return true;
+                    }
+                    layer.open({
+                        content: ret.msg,
+                        style: 'background-color: rgba(0,0,0,.6); text-align:center; color:#fff; border:none; font-size:20px; letter-spacing: 4px; line-height: 200%;max-width: 300px',
+                        time: 2
+                    });
+                },'json')
+            }
+            else {
+                layer.open({
+                    content: ret.msg,
+                    style: 'background-color: rgba(0,0,0,.6); text-align:center; color:#fff; border:none; font-size:20px; letter-spacing: 4px; line-height: 200%;max-width: 300px',
+                    time: 2
+                });
+            }
+        },'json')
+    })
+</script>
